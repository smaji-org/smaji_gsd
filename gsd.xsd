<xs:complexType name="point">
  <xs:annotation>
    <xs:documentation>
      coordinate point
    </xs:documentation>
  </xs:annotation>
  <xs:attribute name="x" type="xs:decimal" use="required"/>
  <xs:attribute name="y" type="xs:decimal" use="required"/>
</xs:complexType>

<xs:complexType name="point_adjust">
  <xs:annotation>
    <xs:documentation>
      coordinate point adjust
    </xs:documentation>
  </xs:annotation>
  <xs:choice>
    <xs:element name="point" type="point"/>
    <xs:element name="auto"/>
  </xs:choice>
</xs:complexType>

<xs:complexType name="length">
  <xs:annotation>
    <xs:documentation>
      coordinate point
