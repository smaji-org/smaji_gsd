    <xs:enumeration value="utj"/>
    <xs:enumeration value="vaj"/>
    <xs:enumeration value="va"/>
    <xs:enumeration value="vcj"/>
    <xs:enumeration value="vc"/>
    <xs:enumeration value="vh"/>
    <xs:enumeration value="vhtj"/>
